        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'Siebel Sensor: SIEBEL_ORDERMANAGEMENT_INCORRECTDUEDATE - Weekly' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, s.KEY_DATA, s.EVENT_RAISED FROM (SELECT  'ORDERMANAGEMENT INCORRECTDUEDATE ' || ' COUNT= ' || COUNT(*) KEY_DATA,  'Incorrect Due Date At Order Line Item Level (Doc ID 2632713.1)' AS SOLUTION,  count(*) as EVENT_RAISED  FROM SIEBEL.S_RR_BUSCOMP WHERE NAME = 'Order Entry - Line Items'   and COMPILED_OBJ_DEFN like '%ParentFieldValue(""Requested Ship Date"") > Today()%'   and REPOSITORY_ID = (SELECT ROW_ID from SIEBEL.S_REPOSITORY where name = 'Siebel Repository')  and (SELECT   count(*)  FROM SIEBEL.S_RR_BUSCOMP  WHERE NAME = 'Order Entry - Orders' and COMPILED_OBJ_DEFN like '%Requested Ship Date%'  and REPOSITORY_ID = (SELECT ROW_ID from SIEBEL.S_REPOSITORY where name = 'Siebel Repository')   and (substr(COMPILED_OBJ_DEFN,instr(COMPILED_OBJ_DEFN,'Requested Ship Date')+19,   (instr(COMPILED_OBJ_DEFN,'Field',(instr(COMPILED_OBJ_DEFN,'Requested Ship Date')),2)-(instr(COMPILED_OBJ_DEFN,'Requested Ship Date')+19)))) like '%DTYPE_UTCDATETIME%') > 0) s]]>
        </SQLQuery>
